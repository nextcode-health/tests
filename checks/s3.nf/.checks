set -e

#
# run normal mode
#
$NXF_RUN | tee stdout

[[ `grep INFO .nextflow.log | grep -c 'Submitted process'` == 1 ]] || false
cmp stdout .expected || false



#
# run resume mode
#
$NXF_RUN -resume | tee stdout

[[ `grep INFO .nextflow.log | grep -c 'Cached process'` == 1 ]] || false
cmp stdout .expected || false
