set -e

#
# run normal mode
#
echo ''
$NXF_RUN | tee stdout

[[ `grep 'INFO' .nextflow.log | grep -c 'Submitted process > sayhello (1)'` == 1 ]] || false
[[ `grep -c 'Hello world!' stdout` == 1 ]] || false


#
# RESUME mode
#
echo ''
$NXF_RUN -resume | tee stdout

[[ `grep 'INFO' .nextflow.log | grep -c 'Cached process > sayhello (1)'` == 1 ]] || false
[[ `grep -c 'Hello world!' stdout` == 1 ]] || false